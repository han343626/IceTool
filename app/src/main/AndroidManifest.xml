<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ice.icetool">

    <application
        android:name=".IceApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="${app_name}"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".item.ControlActivity" />
        <activity android:name=".item.BdActivity" />
        <activity android:name=".item.RefreshListActivity" />
        <activity android:name=".item.NoteActivity" />
        <activity android:name=".item.SwitchAndShop" />
    </application>

</manifest>