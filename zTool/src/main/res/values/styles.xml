<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="BottomDialog" parent="@android:style/Theme.Dialog">

        <!-- 背景透明 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- 浮于Activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- Dialog以外的区域模糊效果 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 半透明 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- Dialog进入及退出动画 -->
        <item name="android:windowAnimationStyle">@style/DialogAnimation</item>
    </style>
    <!-- ActionSheet进出动画 -->
    <style name="DialogAnimation" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/dialog_in</item>
        <item name="android:windowExitAnimation">@anim/dialog_out</item>
    </style>

    <style name="CustomDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>

    <style name="CustomProgressDialog" parent="@style/CustomDialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="ActionSheetAnimation" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/push_up_in</item>
        <item name="android:windowExitAnimation">@anim/push_up_out</item>
    </style>

    <style name="item_row">
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingStart">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:paddingEnd">5dp</item>
    </style>

    <style name="item_common_title_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">#a2caee</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:paddingEnd">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingStart">10dp</item>
    </style>

    <style name="item_common_title_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/font_level_1</item>
        <item name="android:textSize">@dimen/title_font_size_3</item>
    </style>

    <style name="item_text_common">
        <item name="android:layout_height">52dp</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:minWidth">80dp</item>
        <item name="android:singleLine">false</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="item_edit_des">
        <item name="android:gravity">left|top</item>
        <item name="android:inputType">textMultiLine</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:textColor">#4f4f4f</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="item_edit_common">
        <item name="android:gravity">center|left</item>
        <item name="android:inputType">textMultiLine</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:textColor">#4f4f4f</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="item_button_common">
        <item name="android:layout_height">45dp</item>
        <item name="android:background">@drawable/css_button_col_theme</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/top_bar_title_color</item>
    </style>

    <style name="item_edit_common_none">
        <item name="android:gravity">center_vertical</item>
